	"keywords": ["laravel", "lumen", "fingerprint", "secugen", "hamster", "matching", "recognition"],
	"license": "MIT",
	"authors": [
		{
			"name": "Boris KremenoviÄ‡",
			"email": "info@boriskremenovic.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"illuminate/support": "5.*",
		"illuminate/session": "5.*",
		"illuminate/events": "5.*"
	},
	"autoload": {
